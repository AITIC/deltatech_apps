<?xml version="1.0" encoding="utf-8"?>
<openerp>

    <data>

        <record id="view_service_configuration" model="ir.ui.view">
            <field name="name">service settings</field>
            <field name="model">service.config.settings</field>
            <field name="inherit_id" ref="deltatech_service.view_service_configuration"/>

            <field name="arch" type="xml">

                <xpath expr="//group[@id='access']" position="inside">

                    <div>
                        <div>
                            <field name="group_use_service_plan" class="oe_inline"/>
                            <label for="group_use_service_plan"/>
                        </div>
                    </div>

                </xpath>
            </field>
        </record>

    </data>


</openerp>
